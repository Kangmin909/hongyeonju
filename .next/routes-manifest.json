{
  "version": 3,
  "pages404": true,
  "appType": "hybrid",
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "priority": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    }
  ],
  "headers": [],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [],
    "fallback": []
  },
  "dynamicRoutes": [
    {
      "page": "/exhibition/[id]",
      "regex": "^/exhibition/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/exhibition/(?<nxtPid>[^/]+?)(?:/)?$"
    }
  ],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/about",
      "regex": "^/about(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/about(?:/)?$"
    },
    {
      "page": "/api/getAbout",
      "regex": "^/api/getAbout(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getAbout(?:/)?$"
    },
    {
      "page": "/api/getCV1",
      "regex": "^/api/getCV1(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getCV1(?:/)?$"
    },
    {
      "page": "/api/getCV2",
      "regex": "^/api/getCV2(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getCV2(?:/)?$"
    },
    {
      "page": "/api/getExhibition",
      "regex": "^/api/getExhibition(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getExhibition(?:/)?$"
    },
    {
      "page": "/api/getHome",
      "regex": "^/api/getHome(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getHome(?:/)?$"
    },
    {
      "page": "/api/getThumbnail",
      "regex": "^/api/getThumbnail(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getThumbnail(?:/)?$"
    },
    {
      "page": "/api/getWorks",
      "regex": "^/api/getWorks(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/getWorks(?:/)?$"
    },
    {
      "page": "/api/notionImageSync",
      "regex": "^/api/notionImageSync(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/api/notionImageSync(?:/)?$"
    },
    {
      "page": "/cv",
      "regex": "^/cv(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/cv(?:/)?$"
    },
    {
      "page": "/exhibition",
      "regex": "^/exhibition(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/exhibition(?:/)?$"
    },
    {
      "page": "/sitemap.xml",
      "regex": "^/sitemap\\.xml(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/sitemap\\.xml(?:/)?$"
    },
    {
      "page": "/works",
      "regex": "^/works(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/works(?:/)?$"
    }
  ],
  "dataRoutes": [],
  "rsc": {
    "header": "rsc",
    "varyHeader": "rsc, next-router-state-tree, next-router-prefetch, next-router-segment-prefetch",
    "prefetchHeader": "next-router-prefetch",
    "didPostponeHeader": "x-nextjs-postponed",
    "contentTypeHeader": "text/x-component",
    "suffix": ".rsc",
    "prefetchSegmentHeader": "next-router-segment-prefetch",
    "prefetchSegmentSuffix": ".segment.rsc",
    "prefetchSegmentDirSuffix": ".segments",
    "clientParamParsing": false,
    "dynamicRSCPrerender": false
  },
  "rewriteHeaders": {
    "pathHeader": "x-nextjs-rewritten-path",
    "queryHeader": "x-nextjs-rewritten-query"
  }
}